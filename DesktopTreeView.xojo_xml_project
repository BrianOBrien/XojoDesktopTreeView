 <Method>
  <ItemName>Flatten</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>647665663</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub Flatten(n As TreeNode, depth As Integer)</SourceLine>
   <SourceLine>Var r As New RowInfo</SourceLine>
   <SourceLine>r.Node = n</SourceLine>
   <SourceLine>r.Depth = depth</SourceLine>
   <SourceLine>r.HasChildren = n.HasChildren</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If r.HasChildren Then</SourceLine>
   <SourceLine>If mExpanded.HasKey(n.Path) Then</SourceLine>
   <SourceLine>r.Expanded = mExpanded.Value(n.Path).BooleanValue</SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>r.Expanded = False</SourceLine>
   <SourceLine>mExpanded.Value(n.Path) = False</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>mRows.Add(r)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If r.HasChildren And r.Expanded Then</SourceLine>
   <SourceLine>For Each c As TreeNode In n.Children</SourceLine>
   <SourceLine>Flatten(c, depth + 1)</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>33</ItemFlags>
  <IsShared>0</IsShared>
