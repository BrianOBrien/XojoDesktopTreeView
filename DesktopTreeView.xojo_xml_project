 <Method>
  <ItemName>FromVariant</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>492965887</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function FromVariant(name As String, v As Variant, parentPath As String) As TreeNode</SourceLine>
   <SourceLine>Var n As New TreeNode(name, "", parentPath)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If v.IsNull Then</SourceLine>
   <SourceLine>n.Value = "null"</SourceLine>
   <SourceLine>Return n</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If v IsA Dictionary Then</SourceLine>
   <SourceLine>Var d As Dictionary = Dictionary(v)</SourceLine>
   <SourceLine>For Each k As Variant In d.Keys</SourceLine>
   <SourceLine>n.Children.Add(FromVariant(k.StringValue, d.Value(k), n.Path))</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine>Return n</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>If v.IsArray Then</SourceLine>
   <SourceLine>Var arr() As Variant = v</SourceLine>
   <SourceLine>For i As Integer = 0 To arr.LastIndex</SourceLine>
   <SourceLine>Var idx As String = "[" + i.ToString + "]"</SourceLine>
   <SourceLine>n.Children.Add(FromVariant(idx, arr(i), n.Path))</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine>Return n</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>n.Value = v.StringValue</SourceLine>
   <SourceLine>Return n</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>name As String, v As Variant, parentPath As String</ItemParams>
  <ItemResult>TreeNode</ItemResult>
 </Method>
