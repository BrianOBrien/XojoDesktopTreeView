   <SourceLine>// Expand root by default (once)</SourceLine>
   <SourceLine>If Not mExpanded.HasKey(mRoot.Path) Then</SourceLine>
   <SourceLine>mExpanded.Value(mRoot.Path) = True</SourceLine>
   <SourceLine>End If</SourceLine>
   <SourceLine></SourceLine>
