   <ItemName>PaintCellText</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>154738687</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Function PaintCellText(g as Graphics, row as Integer, column as Integer, x as Integer, y as Integer) As Boolean</SourceLine>
    <SourceLine>If row &lt; 0 Or row &gt; mRows.LastIndex Then Return False</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Var r As RowInfo = mRows(row)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If row = LB.SelectedRowIndex Then</SourceLine>
    <SourceLine>g.DrawingColor = TreeSelectedTextColor</SourceLine>
    <SourceLine>Else</SourceLine>
    <SourceLine>g.DrawingColor = TreeTextColor</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Var padLeft As Integer = 6</SourceLine>
    <SourceLine>Var indentPx As Integer = r.Depth * 16</SourceLine>
    <SourceLine>Var drawX As Integer = x + padLeft + indentPx</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Var twisty As String = If(r.HasChildren, If(r.Expanded, "▼ ", "▶ "), "  ")</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Var label As String</SourceLine>
    <SourceLine>If r.HasChildren Then</SourceLine>
    <SourceLine>label = r.Node.Name</SourceLine>
    <SourceLine>Else</SourceLine>
    <SourceLine>label = r.Node.Name + ": " + r.Node.Value</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>g.DrawText(twisty + label, drawX, y)</SourceLine>
    <SourceLine>Return True</SourceLine>
    <SourceLine>End Function</SourceLine>
   </ItemSource>
  </HookInstance>
  <HookInstance>
   <ItemName>MouseDown</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>1785759743</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Function MouseDown(x As Integer, y As Integer) As Boolean</SourceLine>
    <SourceLine>Var row As Integer = LB.RowFromXY(x, y)</SourceLine>
    <SourceLine>If row &lt; 0 Or row &gt; mRows.LastIndex Then Return False</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Var r As RowInfo = mRows(row)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>// Toggle only if the user clicked near the "twisty" area</SourceLine>
    <SourceLine>Var indentPx As Integer = r.Depth * 16</SourceLine>
    <SourceLine>Var twistyLeft As Integer = indentPx</SourceLine>
    <SourceLine>Var twistyRight As Integer = indentPx + 18</SourceLine>
    <SourceLine>Var clickedTwisty As Boolean = (x &gt;= twistyLeft And x &lt;= twistyRight)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If r.HasChildren And clickedTwisty Then</SourceLine>
    <SourceLine>mExpanded.Value(r.Node.Path) = Not r.Expanded</SourceLine>
    <SourceLine>Rebuild</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>// Select row (so SelectionChanged fires + highlight updates)</SourceLine>
    <SourceLine>LB.SelectedRowIndex = row</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>RaiseEvent NodeSelected(r.Node.Path)</SourceLine>
    <SourceLine>Return True</SourceLine>
    <SourceLine>End Function</SourceLine>
   </ItemSource>
  </HookInstance>
  <HookInstance>
   <ItemName>SelectionChanged</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>937480191</PartID>
