 </HookInstance>
 <Method>
  <ItemName>BuildAndLoadBigTree</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1418356735</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub BuildAndLoadBigTree()</SourceLine>
   <SourceLine>// Build a big nested structure safely (Dictionary + Variant arrays)</SourceLine>
   <SourceLine>Var root As New Dictionary</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// System branch</SourceLine>
   <SourceLine>Var sys As New Dictionary</SourceLine>
   <SourceLine>sys.Value("Host") = "piFiveAlive"</SourceLine>
   <SourceLine>Var os As New Dictionary</SourceLine>
   <SourceLine>os.Value("Name") = "Debian"</SourceLine>
   <SourceLine>os.Value("Version") = "13"</SourceLine>
   <SourceLine>os.Value("Arch") = "arm64"</SourceLine>
   <SourceLine>sys.Value("OS") = os</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var net As New Dictionary</SourceLine>
   <SourceLine>Var ifaces() As Variant</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var eth0 As New Dictionary</SourceLine>
   <SourceLine>eth0.Value("Name") = "eth0"</SourceLine>
   <SourceLine>eth0.Value("IPv4") = "192.168.1.50"</SourceLine>
   <SourceLine>eth0.Value("MAC") = "B8:27:EB:12:34:56"</SourceLine>
   <SourceLine>Var routes() As Variant</SourceLine>
   <SourceLine>Var r0 As New Dictionary</SourceLine>
   <SourceLine>r0.Value("Dest") = "0.0.0.0/0"</SourceLine>
   <SourceLine>r0.Value("GW") = "192.168.1.1"</SourceLine>
   <SourceLine>routes.Add(r0)</SourceLine>
   <SourceLine>eth0.Value("Routes") = routes</SourceLine>
   <SourceLine>ifaces.Add(eth0)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var wlan0 As New Dictionary</SourceLine>
   <SourceLine>wlan0.Value("Name") = "wlan0"</SourceLine>
   <SourceLine>wlan0.Value("IPv4") = "192.168.1.77"</SourceLine>
   <SourceLine>wlan0.Value("MAC") = "DC:A6:32:AA:BB:CC"</SourceLine>
   <SourceLine>wlan0.Value("SSID") = "LabNet"</SourceLine>
   <SourceLine>ifaces.Add(wlan0)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>net.Value("Interfaces") = ifaces</SourceLine>
   <SourceLine>sys.Value("Network") = net</SourceLine>
   <SourceLine>root.Value("System") = sys</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// DICOM-ish branch (deep nesting)</SourceLine>
   <SourceLine>Var dicom As New Dictionary</SourceLine>
   <SourceLine>dicom.Value("AET") = "MWLSCP"</SourceLine>
   <SourceLine>dicom.Value("Port") = 104</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var ts() As Variant</SourceLine>
   <SourceLine>ts.Add("1.2.840.10008.1.2")</SourceLine>
   <SourceLine>ts.Add("1.2.840.10008.1.2.1")</SourceLine>
   <SourceLine>ts.Add("1.2.840.10008.1.2.2")</SourceLine>
   <SourceLine>dicom.Value("TransferSyntaxes") = ts</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var peers() As Variant</SourceLine>
   <SourceLine>Var p1 As New Dictionary</SourceLine>
   <SourceLine>p1.Value("AET") = "MODALITY1"</SourceLine>
   <SourceLine>p1.Value("Host") = "192.168.1.120"</SourceLine>
   <SourceLine>p1.Value("Port") = 104</SourceLine>
   <SourceLine>Var p1svc() As Variant</SourceLine>
   <SourceLine>p1svc.Add("MWL")</SourceLine>
   <SourceLine>p1svc.Add("MPPS")</SourceLine>
   <SourceLine>p1.Value("Services") = p1svc</SourceLine>
   <SourceLine>peers.Add(p1)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var p2 As New Dictionary</SourceLine>
   <SourceLine>p2.Value("AET") = "PACS1"</SourceLine>
   <SourceLine>p2.Value("Host") = "192.168.1.10"</SourceLine>
   <SourceLine>p2.Value("Port") = 11112</SourceLine>
   <SourceLine>Var p2svc() As Variant</SourceLine>
   <SourceLine>p2svc.Add("CSTORE")</SourceLine>
   <SourceLine>p2svc.Add("CFIND")</SourceLine>
   <SourceLine>p2svc.Add("CMOVE")</SourceLine>
   <SourceLine>p2.Value("Services") = p2svc</SourceLine>
   <SourceLine>peers.Add(p2)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dicom.Value("Peers") = peers</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Patients -&gt; Studies -&gt; Series -&gt; Instances -&gt; Frames</SourceLine>
   <SourceLine>Var patients() As Variant</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>For patientIndex As Integer = 1 To 5</SourceLine>
   <SourceLine>Var pat As New Dictionary</SourceLine>
   <SourceLine>pat.Value("PatientID") = "P" + patientIndex.ToString("000")</SourceLine>
   <SourceLine>pat.Value("PatientName") = If(patientIndex Mod 2 = 0, "Bob^^^", "Alice^^^")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var studies() As Variant</SourceLine>
   <SourceLine>For studyIndex As Integer = 1 To 3</SourceLine>
   <SourceLine>Var study As New Dictionary</SourceLine>
   <SourceLine>study.Value("StudyInstanceUID") = "1.2.3.4." + patientIndex.ToString + "." + studyIndex.ToString</SourceLine>
   <SourceLine>study.Value("StudyDescription") = "Study " + studyIndex.ToString</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var seriesList() As Variant</SourceLine>
   <SourceLine>For seriesIndex As Integer = 1 To 3</SourceLine>
   <SourceLine>Var series As New Dictionary</SourceLine>
   <SourceLine>series.Value("SeriesInstanceUID") = "1.2.3.4." + patientIndex.ToString + "." + studyIndex.ToString + "." + seriesIndex.ToString</SourceLine>
   <SourceLine>series.Value("Modality") = If(seriesIndex Mod 2 = 0, "CT", "CR")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var instances() As Variant</SourceLine>
   <SourceLine>For instIndex As Integer = 1 To 5</SourceLine>
   <SourceLine>Var inst As New Dictionary</SourceLine>
   <SourceLine>inst.Value("SOPInstanceUID") = series.Value("SeriesInstanceUID").StringValue + "." + instIndex.ToString</SourceLine>
   <SourceLine>inst.Value("InstanceNumber") = instIndex</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var frames() As Variant</SourceLine>
   <SourceLine>For frameIndex As Integer = 1 To 3</SourceLine>
   <SourceLine>Var fr As New Dictionary</SourceLine>
   <SourceLine>fr.Value("Frame") = frameIndex</SourceLine>
   <SourceLine>fr.Value("Checksum") = "chk-" + patientIndex.ToString + "-" + studyIndex.ToString + "-" + seriesIndex.ToString + "-" + instIndex.ToString + "-" + frameIndex.ToString</SourceLine>
   <SourceLine>frames.Add(fr)</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine>inst.Value("Frames") = frames</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>instances.Add(inst)</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>series.Value("Instances") = instances</SourceLine>
   <SourceLine>seriesList.Add(series)</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>study.Value("Series") = seriesList</SourceLine>
   <SourceLine>studies.Add(study)</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>pat.Value("Studies") = studies</SourceLine>
   <SourceLine>patients.Add(pat)</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dicom.Value("Patients") = patients</SourceLine>
   <SourceLine>root.Value("DICOM") = dicom</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Load directly (no JSON parsing)</SourceLine>
   <SourceLine>DesktopTreeView1.LoadFromDictionary(root, "Root")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// If you also want to SEE the JSON:</SourceLine>
   <SourceLine>// Var json As String = GenerateJSON(root)</SourceLine>
   <SourceLine>// System.DebugLog(json)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
