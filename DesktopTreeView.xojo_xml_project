 <HookInstance>
  <ItemName>Opening</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1220235263</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub Opening()</SourceLine>
   <SourceLine>//Var json As String = _</SourceLine>
   <SourceLine>//"{""Patient"":[{""PatientID"":""P001"",""Study"":[{""Desc"":""CT""}]}]}"</SourceLine>
   <SourceLine>//DesktopTreeView1.LoadFromJSON(json, "Root") // use your instance name</SourceLine>
   <SourceLine>BuildAndLoadBigTree</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
